{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "target": "es5",
        "outFile": "game.js"
    },
    "include": [
        "Libraries/**/*",
        "Types/**/*",
        "StoryScript/**/!(app)*.ts",
        "StoryScript/app.js"
    ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
